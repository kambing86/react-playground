{
  "name": "react-playground",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "rsbuild build",
    "check": "biome check --write",
    "dev": "concurrently \"rsbuild dev --open\" \"pnpm codegen --watch\" \"pnpm scss --watch\"",
    "format": "biome format --write",
    "preview": "rsbuild preview",
    "codegen": "graphql-codegen --config codegen.ts",
    "scss": "typed-scss-modules src/**/*.module.scss"
  },
  "dependencies": {
    "@kambing86/event-bus-ts": "^0.0.1",
    "@tanstack/react-query": "^5.51.15",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "jotai": "^2.9.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-query": "^6.1.0",
    "@parcel/watcher": "^2.4.1",
    "@rsbuild/core": "1.0.1-beta.6",
    "@rsbuild/plugin-react": "1.0.1-beta.6",
    "@rsbuild/plugin-sass": "1.0.1-beta.6",
    "@rsbuild/plugin-type-check": "1.0.1-beta.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "sass": "^1.77.8",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5.5.4"
  }
